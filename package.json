{
  "name": "react-similarity-network",
  "version": "0.3.0",
  "description": "Similarity graph for Foursquare places",
  "license": "Apache-2.0",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.27",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "@types/d3": "^5.7.2",
    "axios": "^0.19.2",
    "d3": "^5.16.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0",
    "typescript": "^3.7.5",
    "typedoc": "^0.16.10",
    "onchange": "^6.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "cover": "npm test -- --coverage",
    "document": "typedoc ./src --media ./media",
    "pipeline": "npm run cover && npm run document && npm run build",
    "watch": "onchange -i -k './src' './public' -- npm run pipeline"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
